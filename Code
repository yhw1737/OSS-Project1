#include <stdio.h>

int main(){
while(1) {
  int n;
  char a;
  char world[100];
  gets(world);
  for(int i=0;i<5;i++){
    printf("hello !\n");
    printf("%s", world);
  }
  printf("한 번더 출력할까요? >> "); // yes = 0, no = 1
    scanf_s("%d", &n);
    
    if(n<0)
    {
    break;
    }
    
    if(world[0]==NULL)
    {
    printf("배열이 비었습니다.\n");
    }

    if (n == 0)
    {
        for (int i = 0; i < 5; i++) {
            printf("hello !\n");
            printf("%s", world);
        }
    }
    else if (n == 1)
        return 0;
}
}
